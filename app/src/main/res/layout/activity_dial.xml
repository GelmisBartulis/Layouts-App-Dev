<?xml version="1.0" encoding="utf-8"?>


<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#3C3C3C"
    android:gravity="clip_horizontal|center|clip_vertical"
    android:orientation="vertical"
    android:padding="50dp"
    android:stretchColumns="*"
    android:textAlignment="center">


    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:elevation="10dp"
        android:padding="2dp">


        <EditText
            android:id="@+id/placeholder21"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_margin="0dp"
            android:layout_span="3"
            android:paddingLeft="0dp"
            android:paddingRight="0dp"
            android:textSize="25sp"
            android:autofillHints=""
            android:textAlignment="textEnd"
            android:text="(886) 130-6385"
            android:inputType="number" />


    </TableRow>





    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:elevation="10dp"
        android:padding="2dp">

        <TextView
            android:id="@+id/eight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="1"
            android:textAlignment="center"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/nine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="2"
            android:textAlignment="center"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/multiply"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="3"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="3"
            android:textAlignment="center"
            android:textSize="25sp" />

    </TableRow>

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:elevation="10dp"
        android:padding="2dp">


        <TextView
            android:id="@+id/five"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="4"
            android:textAlignment="center"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/six"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="5"
            android:textAlignment="center"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/minus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="3"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="6"
            android:textAlignment="center"
            android:textSize="25sp" />

    </TableRow>

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:elevation="10dp"
        android:padding="2dp">

        <TextView
            android:id="@+id/two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="7"
            android:textAlignment="center"
            android:textSize="25sp" />

        <TextView

            android:id="@+id/three"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="8"
            android:textAlignment="center"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="3"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="3"
            android:textAlignment="center"
            android:textSize="25sp" />

    </TableRow>

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:elevation="10dp"
        android:padding="2dp">


        <TextView
            android:id="@+id/zero"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="*"
            android:textAlignment="center"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/dot"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="0"
            android:textAlignment="center"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/eq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="3"
            android:background="@android:drawable/alert_light_frame"
            android:padding="5dp"
            android:text="#"
            android:textAlignment="center"
            android:textSize="25sp" />

    </TableRow>


</TableLayout>


